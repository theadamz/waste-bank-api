{
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "baseUrl": "./",
    "paths": {
      "@modules/*": ["src/modules/*"],
      "@db/*": ["src/db/*"],
      "@utils/*": ["src/utils/*"],
      "@routes/*": ["src/routes/*"]
    },
    "types": ["bun-types"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  }
}
